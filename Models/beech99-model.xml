<?xml version="1.0"?>

<PropertyList>

 <path>beech99.ac</path>

 <offsets>
  <z-m>-.25</z-m>
  <x-m>0.8</x-m>
  <pitch-deg>0</pitch-deg>
 </offsets>
 
 
 <panel>
        <name>Panel3D</name>
        <path>Aircraft/beech99/beech99-panel3d.xml</path>
        <bottom-left>
            <x-m>-2.240</x-m>
            <y-m>-0.550</y-m>
            <z-m> 0.780</z-m>
        </bottom-left>
        <bottom-right>
            <x-m>-2.240</x-m>
            <y-m> 0.600</y-m>
            <z-m> 0.780</z-m>
        </bottom-right>
        <top-left>
            <x-m>-2.280</x-m>
            <y-m>-0.550</y-m>
            <z-m> 1.111</z-m>
        </top-left>
    </panel>

 <animation>
  <type>select</type>
  <object-name>Propeller_Left</object-name>
  <condition>
   <less-than>
    <property>engines/engine[0]/rpm</property>
    <value>650</value>
   </less-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>Propeller_Left_Fast</object-name>
  <condition>
   <greater-than>
    <property>engines/engine[0]/rpm</property>
    <value>648</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>Propeller_Right</object-name>
  <condition>
   <less-than>
    <property>engines/engine[1]/rpm</property>
