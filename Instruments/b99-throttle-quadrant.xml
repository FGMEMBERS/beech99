  <layer>
   <name>backplate</name>
   <texture>
    <path>Aircraft/beech99/Instruments/Textures/b99-quadrant-256.rgb</path>
    <x1>0.0</x1>
    <y1>0.0</y1>
    <x2>0.7421875</x2>
    <y2>0.3515625</y2>
   </texture>
   <w>150</w>
   <h>60</h>
  </layer>

<!--throttles -->

  <layer>
   <name>throttle 0 handle </name>
