   <type>x-shift</type>
     <offset>-20</offset>
   </transformation>
   </transformations>
  </layer>
  <layer>
   <name>Bezel-Outer</name>
   <texture>
   <path>Aircraft/beech99/Instruments/Textures/b99-led.rgb</path>
