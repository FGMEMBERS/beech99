<PropertyList>
 <name>marker beacon</name>
 <w-base>78</w-base>
 <h-base>22</h-base>

 <layers>
 <layer>
  <name>backplate</name>
  <texture>
   <path>Aircraft/beech99/Instruments/Textures/b99-omi-256.rgb</path>
   <x1>0.0</x1>
   <y1>0.0</y1>
   <x2>0.3359375</x2>
   <y2>0.07421875</y2>
  </texture> 
 </layer> 
<!-- Outer Indicator -->
 <layer>
   <name>lamp</name>
   <type>switch</type>
   <layer>
   <condition>
    <property>/instrumentation/marker-beacon/outer</property>
   </condition>
   <name>on</name>
   <texture>
    <path>Aircraft/beech99/Instruments/Textures/b99-led.rgb</path>
    <x1>0.5</x1>
    <y1>0.75</y1>
    <x2>0.75</x2>
    <y2>1.0</y2>
   </texture>
    <w>12</w>
    <h>12</h>
   <transformations>
   <transformation>
   <type>x-shift</type>
     <offset>-20</offset>
   </transformation>
   </transformations>
  </layer>
   <layer>
   <name>off</name>
   <texture>
    <path>Aircraft/beech99/Instruments/Textures/b99-led.rgb</path>
    <x1>0.25</x1>
    <y1>0.75</y1>
    <x2>0.5</x2>
    <y2>1.0</y2>
   </texture>
    <w>12</w>
    <h>12</h>
   <transformations>
   <transformation>
   <type>x-shift</type>
     <offset>-20</offset>
   </transformation>
   </transformations>
  </layer>
 </layer>
  <layer>
